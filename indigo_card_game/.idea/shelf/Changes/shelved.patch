+        "triangle" -> {
+            val a = readln().toDoubleOrNull()
+            val b = readln().toDoubleOrNull()
+            val c = readln().toDoubleOrNull()
+            if (a!=null && b!=null && c!=null) {
+                val alpha = acos((((b * b) + (c * c)) - (a * a)) / (2 * b * c))
+                (b * c * sin(alpha)) / 2
+            } else {
+                null
+            }
+        }
+        "rectangle" -> {
+            val a = readln().toDoubleOrNull()
