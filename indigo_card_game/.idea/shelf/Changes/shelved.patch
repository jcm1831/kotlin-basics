+    class CardDeck {
+        private val cards = mutableListOf<Card>()
+
+        init {
+            val ranks =
+                buildList {
+                    addAll(listOf("A", "J", "Q", "K"))
+                    addAll(listOf("2", "3", "4", "5", "6", "7", "8", "9", "10"))
+                }
+
+            val suits = listOf('\u2666', '\u2665', '\u2660', '\u2663')
+            for (suit in suits) {
+                for (rank in ranks) {
+                    cards.add(Card(rank, suit.toString()))
+                }
+            }
+            cards.shuffle()
+        }
+
+        fun getCards(numCards: Int): List<Card> {
+            val cardsFromDeck = mutableListOf<Card>()
+            if (numCards in 1..52) {
+                if (numCards <= cards.size) {
+                    for (i in 0 until numCards) {
+                        cardsFromDeck.add(cards.first())
+                        cards.removeAt(0)
+                    }
+                } else {
+                    println("The remaining cards are insufficient to meet the request.")
+                }
+            } else {
+                println("Invalid number of cards.")
+            }
+            return cardsFromDeck
+        }
+
+        fun isNotEmpty() = cards.isNotEmpty()
+        fun isEmpty() = cards.isEmpty()
+    }
