+- name: src/indigo/IndigoCardGame.kt
+  visible: true
+  text: |-
+    package indigo
+
+    class IndigoCardGame {
+        private val deck = CardDeck()
+        private val humanPlayer = HumanPlayer()
+        private val computerPlayer = ComputerPlayer()
+        private var gameState = GameStates.NULL
+        private val cardsOnTable = CardsOnTable()
+
+        init {
+            println("Indigo Card Game")
+            gameState = GameStates.init()
+            if (gameState.isPlayersTurn()) {
+                humanPlayer.playedFirst = true
+                computerPlayer.playedFirst = false
+            } else if (gameState.isComputersTurn()) {
+                humanPlayer.playedFirst = false
+                computerPlayer.playedFirst = true
+            }
+            cardsOnTable.init(deck)
+            dealCards()
+        }
+
+        fun play() {
+            while (gameState.isNotGameOver()) {
+                cardsOnTable.printStatus()
+                playCard()
+                updateCardStatus()
+            }
+            println("Game over.")
+        }
+
+        private fun dealCards() {
+            humanPlayer.addCardsOnHand(deck.getCards(6))
+            computerPlayer.addCardsOnHand(deck.getCards(6))
+        }
+
+        private fun playCard() {
+            if (gameState.isPlayersTurn()) {
+                print("Cards in hand: ")
+                humanPlayer.printCardsOnHand()
+                val chosenCard = humanPlayer.chooseCard()
+                gameState = if (chosenCard != null) {
+                    cardsOnTable.add(humanPlayer.playCard(chosenCard))
+                    updateScore(humanPlayer, computerPlayer)
+                    GameStates.COMPUTERS_TURN
+                } else {
+                    GameStates.GAME_OVER
+                }
+            } else if (gameState.isComputersTurn()) {
+                val chosenCard = 0
+                cardsOnTable.add(computerPlayer.playCard(chosenCard))
+                println("Computer plays ${cardsOnTable.topCard()}")
+
+                updateScore(computerPlayer, humanPlayer)
+                gameState = GameStates.PLAYERS_TURN
+            }
+        }
+
+        private fun updateScore(player: Player, other:Player) {
+            if (cardsOnTable.isWin()) {
+                player.addCardsWon(cardsOnTable.get())
+                player.wonLast = true
+                other.wonLast = false
+                println("$player wins cards")
+                println(
+                    "Score: Player ${humanPlayer.getScore()} - " +
+                            "Computer ${computerPlayer.getScore()}"
+                )
+                println(
+                    "Cards: Player ${humanPlayer.wonCards()} - " +
+                            "Computer ${computerPlayer.wonCards()}"
+                )
+                cardsOnTable.clear()
+            }
+        }
+
+        private fun calculateFinalScore() {
+            // check remaining cards
+            if (humanPlayer.wonLast) {
+                humanPlayer.addCardsWon(cardsOnTable.get())
+            } else if (computerPlayer.wonLast) {
+                computerPlayer.addCardsWon(cardsOnTable.get())
+            } else {
+                if (humanPlayer.playedFirst) {
+                    humanPlayer.addCardsWon(cardsOnTable.get())
+                } else if (computerPlayer.playedFirst) {
+                    computerPlayer.addCardsWon(cardsOnTable.get())
+                }
+            }
+            // calculate final score
+            var humanScore = humanPlayer.getScore()
+            var computerScore = computerPlayer.getScore()
+            if (humanPlayer.wonCards() > computerPlayer.wonCards()) {
+                humanScore += 3
+            } else if (humanPlayer.wonCards() < computerPlayer.wonCards()) {
+                computerScore += 3
+            } else {
+                if (humanPlayer.playedFirst) {
+                    humanScore += 3
+                } else if (computerPlayer.playedFirst) {
+                    computerScore += 3
+                }
+            }
+            println(
+                "Score: Player $humanScore - Computer $computerScore"
+            )
+            println(
+                "Cards: Player ${humanPlayer.wonCards()} - " +
+                        "Computer ${computerPlayer.wonCards()}"
+            )
+        }
+
+        private fun updateCardStatus() {
+            if (deck.isNotEmpty() && humanPlayer.hasEmptyHand() && computerPlayer.hasEmptyHand()) {
+                dealCards()
+            }
+
+            if (deck.isEmpty() && humanPlayer.hasEmptyHand() && computerPlayer.hasEmptyHand()) {
+                cardsOnTable.printStatus()
+                calculateFinalScore()
+                gameState = GameStates.GAME_OVER
+            }
+        }
+    }
