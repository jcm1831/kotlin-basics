+        private var lastTopCard = Card()
+
+        fun init(deck: CardDeck, numCards: Int = 4) {
+            print("Initial cards on the table: ")
+            cardsOnTable.addAll(deck.getCards(numCards))
+            println(cardsOnTable.joinToString(" "))
+        }
