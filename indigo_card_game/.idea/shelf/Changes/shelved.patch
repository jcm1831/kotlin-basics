+    fun checkOIfValidCardsInHand(cardsStr: String, numOfCards: Int): Boolean {
+        val cards = cardsStr.split(" ")
+        val cardRegex = "([1-6])\\)(A|[2-9]|10|J|Q|K)(♦|♥|♠|♣)".toRegex()
+        for (card in cards) if (!card.matches(cardRegex)) {
