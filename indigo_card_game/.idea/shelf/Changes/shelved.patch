+        fun printStatus() {
+            println()
+            if (cardsOnTable.isEmpty()) {
+                println("No cards on the table")
+            } else {
+                println("${cardsOnTable.size} cards on the table, and the top " +
+                        "card is ${topCard()}")
+            }
+        }
+
+        fun add(card: Card) {
+            if (cardsOnTable.isNotEmpty()) lastTopCard = topCard()
+            cardsOnTable.add(card)
